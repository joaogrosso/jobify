    <%- include('header') %>
    
        
    
        <h2>Admin categorias:
            <a href='/admin/categoria/nova/'>Nova categoria</a>
        
        </h2>
        <div>
            <ul>
                <% categorias.forEach( categoria => { %>
                <li>
                <%- categoria.categoria %>
                <a href='/admin/categorias/delete/<%- categoria.id %> '>Excluir</a></li>
                <a href='/admin/categorias/editar/<%- categoria.id %> '>Editar</a></li>
                <% }) %>
            </ul>
        </div>

    <%- include('footer') %>